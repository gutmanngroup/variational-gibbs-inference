{
    "seed": 20190508,
    "exp_group": "toy_set3_mcar/learning_experiments/2",
    "experiment_name": "fa_svariational_cdi_2",
    "dataset": "toy_set3",
    "imputation": "mean",
    "input_missing_vectors": "none",
    // Whether to set the missing variable to zero before passing to the shared network
    "set_missing_zero": false,
    // Whether to impute the dataset with a posterior sample or mean
    "sample_imputation": true,
    // Sample all missing variables or 1 at a time
    "sample_all": true,
    // When to switch ELBO from sample_all
    "switch_sampling_epoch": 999999999999,
    // When to switch imputation from sample_all
    "switch_imputation_epoch": 0,
    // posterior approximation method used in CDI
    "posterior_approximation_method": "variational",
    // Variational model used - individual per distribution or partially shared
    "variational_model": "shared",
    "activation": "lrelu",
    // num_samples is how many samples CDI should use to approximate the expectation
    "num_samples": 5,
    "missingness": 0.3333,
    "missingness_type": "dataset_ratio",
    // Wait imputation_delay epochs before imputing values in the dataset
    "imputation_delay": 0,
    "batch_size": 2048,
    "num_epochs": 400,
    "entropy_coeff": 1.0,
    "model_learning_rate": 1e-1,
    "var_learning_rate": 1e-1,
    "model_weight_decay_coefficient": 0.0,
    "var_weight_decay_coefficient": 0.0,
    "input_len": 6,
    // latent dimension of the Factor Analysis model
    "latent_dim": 2,
    // hidden layer dimensions of the variational model
    "prenet_layer_dims": [10],
    "outnet_layer_dims": [6]
}