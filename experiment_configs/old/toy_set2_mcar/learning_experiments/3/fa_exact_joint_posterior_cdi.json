{
    "seed": 20190508,
    "exp_group": "toy_set2_mcar/learning_experiments/3",
    "experiment_name": "fa_exact_joint_posterior_cdi",
    "dataset": "toy_set2",
    "imputation": "mean",
    // Whether to impute the dataset with a posterior sample or mean
    "sample_imputation": true,
    // Sample all missing variables or 1 at a time
