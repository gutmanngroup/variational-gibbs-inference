    "num_samples": 5,
    "missingness": 0.5,
    "missingness_type": "dataset_ratio",
    // Wait imputation_delay epochs before imputing values in the dataset
    "imputation_delay": 0,
    "batch_size": 2048,
    "num_epochs": 400,
    "learning_rate": 1e-1,
    "weight_decay_coefficient": 0.0,
    "input_len": 6,
    // latent dimension of the Factor Analysis model
    "latent_dim": 2
}